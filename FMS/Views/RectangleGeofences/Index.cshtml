@model IEnumerable<FMS.Models.EfModels.RectangleGeofence>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.North)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.East)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.West)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.South)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Geofence)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.North)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.East)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.West)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.South)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Geofence.GeofenceId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
